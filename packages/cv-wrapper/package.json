{
  "name": "cv-wrapper",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "My Vue based wrapper application for serving the CV content dynamically and providing additional features",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --build --force && vite build",
    "lint": "concurrently --kill-others-on-fail -n prettier,eslint,type-check 'prettier . --check' 'eslint' 'vue-tsc --noEmit'",
    "fix": "prettier . --write && eslint --fix"
  },
  "dependencies": {
    "primeicons": "7.0.0",
    "primevue": "3.52.0",
    "vue": "3.4.27"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.10.3",
    "@tsconfig/node20": "20.1.4",
    "@types/node": "20.14.2",
    "@vitejs/plugin-vue": "5.0.5",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/tsconfig": "0.5.1",
    "eslint": "8.56.0",
    "eslint-plugin-vue": "9.26.0",
    "typescript": "5.4.5",
    "unplugin-vue-components": "0.27.0",
    "vite": "5.2.13",
    "vue-tsc": "2.0.19"
  }
}

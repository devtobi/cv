{
  "extends": "@tsconfig/node-lts/tsconfig.json",
  "include": [
    "vite.config.*",
    "./package.json",
    "../cv-content/package.json",
    "../../package.json"
  ],
  "compilerOptions": {
    "composite": true,
    "noEmit": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "types": ["node"],
    "resolveJsonModule": true
  }
}
